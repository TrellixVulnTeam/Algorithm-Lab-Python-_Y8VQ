def biggerTwo(arr1,arr2):

    sum1 = 0
    sum1 = arr1[0] + arr1[1]

    sum2 = 0
    sum2 = arr2[0] + arr2[1]

    if (sum1 > sum2):
        print(arr1[0],arr1[1])
    else:
        print(arr2[0],arr2[1])

biggerTwo(arr1=[1,2],arr2=[3,4])
biggerTwo(arr1=[1,2],arr2=[0,1])
biggerTwo(arr1=[4,5],arr2=[3,4])